<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    
    
	<mime-mapping>
		<extension>json</extension>
		<mime-type>text/json; charset=UTF-8</mime-type>
	</mime-mapping>
	
	
	<filter>
        <filter-name>logFilter</filter-name>
        <filter-class>com.axa.server.base.filter.LogFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>logFilter</filter-name>
        <url-pattern>*</url-pattern>
    </filter-mapping>
    
    
    <servlet>
        <servlet-name>gcm</servlet-name>
        <servlet-class>com.axa.server.base.servlet.GcmServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gcm</servlet-name>
        <url-pattern>/gcm</url-pattern>
    </servlet-mapping>
    
    
   	<servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.axa.server.base.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login/*</url-pattern>
    </servlet-mapping>
    

    <servlet>
        <servlet-name>users</servlet-name>
        <servlet-class>com.axa.server.base.servlet.UsersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>users</servlet-name>
        <url-pattern>/users/*</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>boosts</servlet-name>
        <servlet-class>com.axa.server.base.servlet.BoostsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>boosts</servlet-name>
        <url-pattern>/boosts/*</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>recipes</servlet-name>
        <servlet-class>com.axa.server.base.servlet.RecipesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>recipes</servlet-name>
        <url-pattern>/recipes/*</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>cities</servlet-name>
        <servlet-class>com.axa.server.base.servlet.CitiesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cities</servlet-name>
        <url-pattern>/cities/*</url-pattern>
    </servlet-mapping>
    

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    
</web-app>
